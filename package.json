    "@supabase/supabase-js": "^1.35.3",
    "adm-zip": "^0.5.9",
    "cli-progress": "3.11.0",
    "commander": "9.2.0",
    "form-data": "^4.0.0",
    "fs-extra": "10.1.0",
    "mime": "^3.0.0",
    "node-stream-zip": "^1.15.0",
    "prettyjson": "^1.2.5",
    "semver": "^7.3.7",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@types/adm-zip": "0.5.0",
    "@types/cli-progress": "^3.11.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mime": "^2.0.3",
    "@types/node": "^17.0.31",
    "@types/prettyjson": "^0.0.30",
    "@types/semver": "^7.3.10",
