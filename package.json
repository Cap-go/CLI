    "eslint-plugin-prettier": "^4.0.0",
    "git-format-staged": "3.0.0",
    "husky": "^7.0.4",
    "nodemon": "2.0.16",
    "pkg": "5.6.0",
    "prettier": "2.6.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "4.0.0",
